            form=(FINS_NET_INC(rbase,speriod,eperiod,frq,rel_item,curn,units)-FINS_RCGL_XCGT(rbase,speriod,eperiod,frq,rel_item,curn,units,rbase_add))//FF_EMP_NUM(rbase,speriod,eperiod,frq,rel_item,curn,units); 
            data_units="RATIO";
        case "CALC_CIA_EMP_NUM": 
            form=FINS_TOT_CASH_IA(rbase,speriod,eperiod,frq,rel_item,curn,units,rbase_add)//FF_EMP_NUM(rbase,speriod,eperiod,frq,rel_item,curn,units);
            data_units="RATIO";
\\TL
        case "CALC_OTN_EMP_NUM": 
            form=FINS_OIE_TITLE_NET(rbase,speriod,eperiod,frq,rel_item,curn,units)//FF_EMP_NUM(rbase,speriod,eperiod,frq,rel_item,curn,units);       
            data_units="RATIO";
        case "CALC_TITLE_EP_EMP_NUM":  
            form=FINS_TITLE_EP(rbase,speriod,eperiod,frq,rel_item,curn,units)//FF_EMP_NUM(rbase,speriod,eperiod,frq,rel_item,curn,units);       
            data_units="RATIO";
        case "CALC_CIA_CASH_ST_EMP_NUM":  
            form=(FINS_TOT_CASH_IA(rbase,speriod,eperiod,frq,rel_item,curn,units,rbase_add)-FINS_CASH_ST(rbase,speriod,eperiod,frq,rel_item,curn,units,rbase_add))//FF_EMP_NUM(rbase,speriod,eperiod,frq,rel_item,curn,units);   
            data_units="RATIO";
*/
    }
    \\Cast formula to string and replace RF/RP quotes
    string form_string=form;
    string_array form_5yh_string=form_5yh;
    
    
    \\Handle @NA Parameters (mainly default versions of rbase_add)
    form_string=STRING_REPLACE(form_string,"@NA,",",");
    form_string=STRING_REPLACE(form_string,"@NA)",")");
    form_5yh_string=STRING_REPLACE(form_5yh_string,"@NA,",",");
    form_5yh_string=STRING_REPLACE(form_5yh_string,"@NA)",")");
    
    \\Create 5YR Historical Formula
    if(ISAV(form_5yh_string)){
        \\
        if(restated){
            int_array period_int=IMC_INDEX(speriod,eperiod);
            \\
            \\Create Parameter Arrays
            string_array sdate=if(period_int>=-4,0,period_int+4);
            string_array edate=sdate;
            string_array yr=if(period_int>=-4,period_int+"YR","-4YR");
            \\
            \\Create an Array of 5YR Historical for As-Is Values
            form_5yh_string=(sdate+STRING_NA)@form_5yh_string;
            foreach(i,sdate){
                form_5yh_string[i]=MULTI_STRING_REPLACE(form_5yh_string[i],CONCAT("[SDATE]","[EDATE]","[YR]"),CONCAT(sdate[i],edate[i],yr[i]));
            }
        }else{
            \\
            \\Create a Single Formula for As-Was Values
            form_5yh_string=STRING_REPLACE(form_5yh_string,"[SDATE]",speriod);
            form_5yh_string=STRING_REPLACE(form_5yh_string,"[EDATE]",eperiod);
            form_5yh_string=STRING_REPLACE(form_5yh_string,"[YR]","0YR");
        }
    }

    \\Validate Data Type and Units
    {
        string_array validate=COMA_FIN_STMT_VALIDATE_DATA_TYPE_AND_UNITS(data_type=data_type,data_units=data_units);
        data_type=GAV(validate,"data_type");
        data_units=GAV(validate,"data_units")@units;
        
    }

    \\Return
    rval.data_type=data_type@"CURRENCY";
    rval.data_units=data_units@units;
    rval.form=form_string;
    rval.form_type=form_type;
    rval.form_5yh=form_5yh_string;
    rval.stat_blacklist=stat_blacklist;

    return rval;
}